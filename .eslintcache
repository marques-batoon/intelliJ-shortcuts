        }\n            else if (set===\"tango\") {\n                return kanji.tango[characters[num]].form\n            }\n        }\n\n        const imiSet = () => {\n            if (set===\"yomu\") {\n                return kanji.yomerebaii[characters[num]].意味\n            }\n            else if (set===\"kaku\") {\n                return kanji.kaku[characters[num]].意味\n            }\n            else if (set===\"tango\") {\n                return kanji.tango[characters[num]].意味\n            }\n        }\n\n\n        return (\n            <React.Fragment>\n                <div className=\"btnContainer\">\n                    <button onClick={this.prevChar} disabled={prevDis}>Prev</button>\n                    <button className={shuffleHide} onClick={this.shuffleArray}>Shuffle</button>\n                    <button onClick={this.nextChar} disabled={nextDis}>Next</button>\n                </div>\n                <h3>{setName}</h3>\n                <div className=\"kanjiContainer\">\n                    <h1 className=\"kanji\">{characters[num]}</h1>\n                </div>\n                <div className=\"center\">\n                    <button onClick={this.imi} className={imiButton}>意味</button>\n                    <h3 className={imiClass}>{formSet()}</h3>\n                </div>\n                <div className=\"center\">\n                    <h3 className={imiClass}>{imiSet()}</h3>\n                </div>\n                <div onKeyUp={this.keyUpHandler}> </div>\n            </React.Fragment>\n        )\n    }\n}\n\nexport default Yomu;","/Users/marquesb/Documents/FrontEnd/_CreateReact/intelliJ/src/kanji.js",[],"/Users/marquesb/Documents/FrontEnd/_CreateReact/intelliJ/src/chapters/unit1_1.js",[],"/Users/marquesb/Documents/FrontEnd/_CreateReact/intelliJ/src/chapters/unit3_2.js",[],"/Users/marquesb/Documents/FrontEnd/_CreateReact/intelliJ/src/chapters/unit2_1.js",[],"/Users/marquesb/Documents/FrontEnd/_CreateReact/intelliJ/src/chapters/unit3_1_sankou.js",[],"/Users